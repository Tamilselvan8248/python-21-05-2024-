def is_prime(num):
    if num <= 1:
        return False
    if num == 2:
        return True
    if num % 2 == 0:
        return False
    for i in range(3, int(num**0.5) + 1, 2):
        if num % i == 0:
            return False
    return True
def print_primes_up_to_n(n):
    for num in range(2, n + 1):
        if is_prime(num):
            print(num, end=" ")
n = 50
print(f"Prime numbers up to {n}:")
print_primes_up_to_n(n)
